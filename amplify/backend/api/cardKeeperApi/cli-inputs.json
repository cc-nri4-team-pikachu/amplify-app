{
  "version": 1,
  "paths": {
    "/users": {
      "name": "/users",
      "lambdaFunction": "usersfunc",
      "permissions": {
        "setting": "open"
      }
    },
    "/users/{userId}/cards": {
      "name": "/users/{userId}/cards",
      "lambdaFunction": "cardList",
      "permissions": {
        "setting": "private",
        "auth": [
          "create",
          "read",
          "update",
          "delete"
        ]
      }
    }
  }
}